<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="$lang$" xml:lang="$lang$"$if(dir)$ dir="$dir$"$endif$>
<head>
  <meta charset="utf-8" />
  <meta name="generator" content="pandoc" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes" />
$for(author-meta)$
  <meta name="author" content="$author$" />
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$" />
$endif$
$if(tags)$
  <meta name="keywords" content="$for(tags)$$tags$$sep$, $endfor$" />
$endif$
$if(description)$
  <meta name="description" content="$description$" />
$else$
  <meta name="description" content="$site_description$" />
$endif$
  <title>$if(title-prefix)$$title-prefix$ :: $endif$$pagetitle$</title>
  <style>
    $styles.html()$
  </style>
  <link rel="alternate" type="application/rss+xml" title="RSS Feed $site_name$" href="/feed.xml" />
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
$for(css)$
  <link rel="stylesheet" href="$css$" />
$endfor$
  <script src="/js/mathjax.config.js" defer></script>
  <script src="/js/tex-mml-chtml.min.js" type="text/javascript" defer></script>
  <!--[if lt IE 9]>
    <script src="/js/html5shiv-printshiv.min.js"></script>
  <![endif]-->
$for(header-includes)$
  $header-includes$
$endfor$
  <script src="/js/main.js" async></script>
</head>
<body>
  <script>
    (function() {
        // Block page until the theme is set
        window.__LS_DARK_THEME_KEY = 'dark_theme';

        const darkTheme = !!localStorage.getItem(window.__LS_DARK_THEME_KEY);

        if (darkTheme) {
            document.documentElement.setAttribute('data-theme', 'dark');
        }
    })();
  </script>
  <div class="box">
$for(include-before)$
$include-before$
$endfor$
${menu()}
<div class="row content">
